{
    "name": "machitgarha/bimoo",
    "description": "Moodle declaration stubs, helping IDEs and static analyzers",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Mohammad Amin Chitgarha",
            "email": "machitgarha@outlook.com",
            "homepage": "https://github.com/machitgarha",
            "role": "Creator"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "php-stubs/generator": "dev-constant-support",
        "symfony/console": "^5.3",
        "webmozart/path-util": "^2.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/machitgarha/php-stubs-generator.git"
        }
    ],
    "autoload": {
        "psr-4": {
